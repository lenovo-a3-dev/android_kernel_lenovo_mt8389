[project]
	description = Kernel for Lenovo A3000/S6000
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Olivier (olivieer)
[access "refs/for/refs/*"]
	read = group user/Olivier (olivieer)
	push = group user/Olivier (olivieer)
	label-Code-Review = -2..+2 group user/Olivier (olivieer)
	label-Verified = -1..+1 group user/Olivier (olivieer)
	rebase = group user/Olivier (olivieer)
	removeReviewer = group user/Olivier (olivieer)
	submit = group user/Olivier (olivieer)
[access "refs/heads/*"]
	read = group user/Olivier (olivieer)
	create = group user/Olivier (olivieer)
	push = +force group user/Olivier (olivieer)
	pushMerge = group user/Olivier (olivieer)
[access "refs/tags/*"]
	push = +force group user/Olivier (olivieer)
	pushTag = group user/Olivier (olivieer)
	pushSignedTag = group user/Olivier (olivieer)
